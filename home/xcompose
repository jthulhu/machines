<Multi_key> <m> <i> <n> <period>	: "∈"		# ELEMENT OF
<Multi_key> <m> <slash> <i> <n>         : "∉"           # NOT ELEMENT OF
<Multi_key> <m> <q> <f>                 : "∀"           # FOR ALL
<Multi_key> <m> <q> <e>                 : "∃"           # EXISTS
<Multi_key> <m> <q> <n> <e>             : "∄"           # NOT EXISTS
<Multi_key> <bar> <equal>               : "⊨"           # MODELS
<Multi_key> <bar> <minus>               : "⊢"           # ENTAILS 
<Multi_key> <slash> <bar> <equal>       : "⊯"           # NOT MODELS
<Multi_key> <m> <s> <u> <m>             : "Σ"           # CAPITAL SIGMA
<Multi_key> <m> <n> <a> <b>		: "∇"   UE2207  # NABLA
<Multi_key> <m> <n> <o> <t>             : "¬"           # LOGICAL NOT

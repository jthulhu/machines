# doubles (additional stroke <q>)
<Multi_key> <g> <q> <t> : "θ"    U03B8    # GREEK SMALL LETTER THETA
<Multi_key> <g> <q> <o> : "ο"    U03BF    # GREEK SMALL LETTER OMICRON
<Multi_key> <g> <q> <p> : "ψ"    U03C8    # GREEK SMALL LETTER PSI
<Multi_key> <g> <q> <s> : "ς"    U03C2    # GREEK SMALL LETTER FINAL SIGMA
<Multi_key> <g> <q> <x> : "ξ"    U03BE    # GREEK SMALL LETTER XI
# alternative lower case
<Multi_key> <g> <q> <e> : "ϵ"	 	  # GREEK SMALL LETTER EPSILON

# Maths symbols
# ~~~~~~~~~~~~~
<Multi_key> <slash> <equal>		: "≠"	U2260	# NOT EQUAL
<Multi_key> <equal> <slash>   		: "≠"	U2260	# NOT EQUAL
<Multi_key> <m> <i> <n> <f>		: "∞"	U221E	# INFINITY
<Multi_key> <m> <i> <f> <f>		: "⇔"		# IFF
<Multi_key> <m> <equal> <greater>	: "⇒"		# IMPLIES
<Multi_key> <m> <less> <equal>		: "⇐"		# FROM
<Multi_key> <m> <s> <q> <r> <t>		: "√"		# SQUARE ROOT
<Multi_key> <m> <2> <r> <t> 		: "√"		# SQUARE ROOT
<Multi_key> <m> <3> <r> <t>		: "∛"		# CUBE ROOT
<Multi_key> <m> <4> <r> <t>		: "∜"		# FORTH ROOT
<Multi_key> <m> <i> <n> <t>		: "∫"		# INTEGRAL
<Multi_key> <m> <i> <n> <period>	: "∈"		# ELEMENT OF
<Multi_key> <m> <R>                     : "ℝ"   U211D   # DOUBLE STRUCK CAPITAL R
<Multi_key> <m> <s> <u> <m>             : "Σ"           # CAPITAL SIGMA


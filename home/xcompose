<Multi_key> <equal> <slash>   		: "≠"	U2260	# NOT EQUAL
<Multi_key> <asciitilde> <equal>        : "≅"           # ISOMORPHIC
<Multi_key> <m> <i> <n> <f>		: "∞"	U221E	# INFINITY
<Multi_key> <m> <i> <f> <f>		: "⇔"		# IFF
<Multi_key> <m> <equal> <greater>	: "⇒"		# IMPLIES
<Multi_key> <m> <less> <equal>		: "⇐"		# FROM
<Multi_key> <m> <s> <q> <r> <t>		: "√"		# SQUARE ROOT
<Multi_key> <m> <2> <r> <t> 		: "√"		# SQUARE ROOT
<Multi_key> <m> <3> <r> <t>		: "∛"		# CUBE ROOT
<Multi_key> <m> <4> <r> <t>		: "∜"		# FORTH ROOT
